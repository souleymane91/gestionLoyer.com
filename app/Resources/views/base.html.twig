{# app/Resources/views/layout.html.twig #}

<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <title>{% block title %}SMB Gestion Loyer{% endblock %}</title>

  {% block stylesheets %}
     <!-- <link rel="stylesheet" href="{{asset('bundles/bootstrap/css/bootstrap.min.css') }}" type="text/css"
media="all" /> -->
     <link rel="stylesheet" href="{{asset('bundles/loyer/css/screen.css') }}" type="text/css"/>
     <!-- <link rel="stylesheet" href="{{asset('bundles/css/style.css') }}" type="text/css"> -->
    <!-- <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css"> -->
  {% endblock %}
</head>

<body>
  <div class="contenant" >
    <div class="row">
      <div id="header">
        <div class="logo"></div>
        <div class="titre">
          <h1>Gestion du loyer</h1>
          
        </div> 
      </div>
    </div>

    <div class="row">
      <div id="contents">
        {% block body %}
        {% endblock %}
      </div>
    </div>

    <hr>

    <footer>
      <p>MBAYE Souleymane © {{ 'now'|date('Y') }}.</p>
      {% block javascripts %}
        {# Ajoutez ces lignes JavaScript si vous comptez vous servir des fonctionnalités du bootstrap Twitter #}
        <script src="{{asset('bundles/bootstrap/js/jquery-2.1.1.min.js') }}"></script>
        <script src="{{asset('bundles/bootstrap/js/bootstrap.js') }}"></script>
      {% endblock %}
    </footer>
  </div>


</body>
</html>